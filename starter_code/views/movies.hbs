
<h1 class="moviesH1">IRONHACK CINEMA</h1>
<p class="paragraph">Click on the movie to check the showtimes!</p>
<div class="movie-box">
  {{#each movies}}
  <div class="flex-box">
    <img src="{{this.image}}" alt="">
    <h2><a href ="/movie/{{this.id}}">{{this.title}}</a></h2>
    <a href="/movie/{{this.id}}" <p>See more</p></a>
  </div>
  {{/each}}
</div>
