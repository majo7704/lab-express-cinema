<div class="movie-detail-container">
  <div>
    <a href="/movies"><p class="backLink">Go back</p></a>
    <img src="{{movie.image}}" alt="">
    <h2>{{movie.title}}</h2>
    <h3>Director: {{movie.director}}</h3>
    <h4>Stars</h4>
    <ul>
      {{#each movie.stars}}
        <li>{{this}}</li><!-- HBS DOCUMENTATION-->
      {{/each}}
    </ul>
    <div>
      <p class="pDescription paragraph">Description {{movie.description}}</p>
    </div>
    <ul class="timeContainer">
      {{#each movie.showtimes}}
        <li>{{this}} |</li>
      {{/each}}
    </ul>
  </div>
</div>