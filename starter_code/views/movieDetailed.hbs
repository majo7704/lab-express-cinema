<div>
  <div>
    <a href="/movies"><p>Go back</p></a>
    <img src="{{movie.image}}" alt="">
    <h2>{{movie.title}}</h2>
    <h3>Director: {{movie.director}}</h3>
    <h4>Stars</h4>
    <ul>
      {{#each movie.stars}}
        <li>{{this}}</li><!-- HBS DOCUMENTATION-->
      {{/each}}
    </ul>
    <p>Description {{movie.description}}</p>
    <ul>
      {{#each movie.showtimes}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
  </div>
</div>